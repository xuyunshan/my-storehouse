    "esm": "^3.2.25",
    "gatsby": "^3.9.1",
    "qs": "^6.10.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "sass": "^1.35.2",
    "swiper": "^6.7.5"
  },
  "devDependencies": {
    "@linkedin/node-mp-bridge": "^1.2.12",
    "@linkedin/pemberly-core": "^2.0.79",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
