    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "postcss-preset-env": "^6.7.0",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.0.5",
    "typescript": "^4.3.5"
