{
  "name": "@linkedin/limatteo-frontend",
  "version": "0.0.1",
  "description": "job events campaign in cn",
  "main": "index.js",
  "scripts": {
    "build": "gridsome build",
    "develop": "gridsome develop",
    "explore": "gridsome explore",
    "upload": "limatteo --publish",
    "test": "npm-run-all lint:* test:*",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint .",
    "test:jest": "jest",
    "postinstall": "which node-mp-bridge || exit 0; node-mp-bridge yarn:postinstall --milestone 1",
    "prepack": "pemberly-core prepack",
    "postpack": "pemberly-core postpack"
  },
  "author": "zifan",
  "license": "UNLICENSED",
  "repository": {
    "url": "ssh://git.corp.linkedin.com:29418/multiproducts/limatteo-frontend.git"
  },
  "devDependencies": {
    "@linkedin/limatteo-cli": "^0.0.1",
    "@linkedin/node-mp-bridge": "^1.2.12",
    "@linkedin/pemberly-core": "^2.0.79",
    "babel-eslint": "^10.1.0",
    "bulma": "^0.9.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^4.3.0",
    "jest": "^26.1.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-pxtorem": "^5.0.0",
    "prettier": "^2.1.2",
    "sass-loader": "^10.0.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "callapp-lib": "^3.5.0",
    "gridsome": "^0.7.0",
    "swiper": "^5.4.5",
    "vue-awesome-swiper": "^4.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "node-mp-bridge yarn:precommit --milestone 1 && git add product-spec.json"
    }
  },
  "volta": {
    "node": "12.13.0",
    "extends": "/usr/local/linkedin/etc/volta/volta.json",
    "yarn": "1.22.10"
  }
}
