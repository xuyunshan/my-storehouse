{
  "name": "learning-web",
  "version": "0.0.0-productspec",
  "description": "This README outlines the details of collaborating on this Ember application. A short introduction of this app could easily go here.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git@gitli.corp.linkedin.com:learning/learning-web.git"
  },
  "scripts": {
    "build": "ember build",
    "format": "prettier --config ./.prettierrc --write \"{app/**,tests/**}/*.{js,scss}\"",
    "format-check": "prettier --config ./.prettierrc --list-different \"{app/**,tests/**}/*.{js,scss}\"",
    "pe-e2e-test": "li-wdio --fabric ei --specs selenium/test/** --launch sg_chrome_50.0",
    "live-test": "li-wdio --fabric prod --specs \"${TESTS:-live-tests/tests/**}\" --launch ${BROWSER:=sg_chrome_61.0} live-tests/wdio.conf.js",
    "start": "ember server",
    "test": "ember exam --split 3 --parallel --launch PhantomJS",
    "test:ci": "ember exam --split 3 --parallel --launch PhantomJS --reporter xunit",
    "test-sync": "ember test",
    "prepublish": "echo 'No prepublish script yet, running anyhow to avoid just init errors'",
    "eslint": "node_modules/.bin/eslint",
    "eslint-app": "npm run eslint app/",
    "eslint-tests": "npm run eslint tests/",
    "docs": "./docs/bin/book-serve.sh",
    "clean-pdsc": "rm -fr node_modules/@linkedin/ember-cli-pdsc-handler/app/codegen",
    "clean-bpr": "rm -rf build && rm -rf dist && rm -rf ~/.just/learning-web",
    "kill-phantom": "ps -ef | grep phantomjs | awk '{print $2}' | xargs sudo kill -9",
    "qd-config": "ps -a | grep play | grep 'http.port' | awk 'match($0,/port=[0-9]+/) {print substr($0, RSTART, RLENGTH)}'",
    "qd-config-linux": "ps -aux | grep play | grep 'http.port' | awk 'match($0,/port=[0-9]+/) {print substr($0, RSTART, RLENGTH);exit;}'",
    "clear-cache": "rm -fr ~/.gradle/ligradle/.npm-cache ~/.npm node_modules",
    "precommit:prettier": "echo 'Prettier-fying staged files...' && ./scripts/prettier-precommit.sh"
  },
  "engines": {
    "node": ">= 6.9.4"
  },
  "ember-addon": {
    "configPath": "ember-config",
    "paths": [
      "lib/head-ssr"
    ]
  },
